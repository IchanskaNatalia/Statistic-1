#include <stdio.h>
#include <math.h>

int main() {
    double a, b, c;
    scanf("%lf %lf %lf", &a, &b, &c);

    if (a == 0) {
        printf("Це не квадратне рівняння\n");
        if (b==0)
          if (c==0 )
        return 0;
    }

    double D = b*b - 4*a*c;

    if (D > 0) {
        double x1 = (-b + sqrt(D)) / (2*a);
        double x2 = (-b - sqrt(D)) / (2*a);
        printf("2 розв'язки: x1 = %.2f, x2 = %.2f\n", x1, x2);
    } 
    else if (D == 0) {
        double x = -b / (2*a);
        printf("1 розв'язок: x = %.2f\n", x);
    } 
    else {
        printf("Розв'язків немає\n");
    }

    return 0;
}
